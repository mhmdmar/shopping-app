{
    "name": "shopping-app-client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "serve:prod": "vue-cli-service serve --mode production",
        "build": "vue-cli-service build --dest ../server/public",
        "build:dev": "vue-cli-service build --mode development --dest ../server/public",
        "build:dev-go": "vue-cli-service build --mode development --dest ../../GoServer/public",
        "test": "npm run test:unit",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "bootstrap-vue": "^2.21.2",
        "core-js": "^3.6.5",
        "fuzzysearch": "^1.0.3",
        "utilly": "^1.0.3",
        "vue": "^2.6.12",
        "vue-loading-overlay": "^3.4.2",
        "vue-router": "^3.2.0",
        "vue-star-rating": "^1.7.0",
        "vuex": "^3.4.0",
        "vuex-persistedstate": "^4.0.0-beta.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.5.0",
        "@vue/cli-plugin-eslint": "^4.5.11",
        "@vue/cli-plugin-router": "^4.5.0",
        "@vue/cli-plugin-unit-jest": "^4.5.0",
        "@vue/cli-plugin-vuex": "^4.5.0",
        "@vue/cli-service": "^4.5.13",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "^1.0.3",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "lint-staged": "^9.5.0",
        "prettier": "^1.19.1",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,vue}": [
            "vue-cli-service lint",
            "git add"
        ]
    }
}
